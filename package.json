{
  "name": "circult_emulator",
  "version": "0.1.0",
  "description": "较为简单的电路模拟器",
  "main": "index.js",
  "scripts": {
    "start": "electron ./index.js --inspect=5858",
    "watch:window": "watchify ./scripts/mainWindowRender.js -t babelify -o ./public/bundle.window.js --release --verbose",
    "build:window": "browserify ./scripts/mainWindowRender.js -o ./public/js/bundle.js -t babelify",
    "watch:core": "watchify ./scripts/mainViewRender.js -t babelify -o ./public/bundle.window.js --release --verbose",
    "build:core": "browserify ./scripts/mainViewRender.js -o ./public/js/bundle.js -t babelify",
    "package": "electron-packager ./ MCBBS-Client --all --overwrite --out=./packaged/ --icon=./public/img/logo.ico",
    "package:windows": "electron-packager ./ MCBBS-Client --platform=win32 --overwrite --out=./packaged/ --icon=./public/img/logo.ico",
    "package:mac": "electron-builder build --mac --config electron-builder.json --publish never",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langyo/circult_emulator.git"
  },
  "author": "langyo",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/langyo/circult_emulator/issues"
  },
  "homepage": "https://github.com/langyo/circult_emulator#readme",
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/system": "^3.0.0-alpha.2",
    "babel-loader": "^8.0.0-beta.0",
    "classnames": "^2.2.6",
    "konva": "^3.2.2",
    "lowdb": "^1.0.0",
    "mdi-material-ui": "^5.10.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.8.4",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.4",
    "react-konva": "^16.8.4",
    "reflux": "^6.4.1",
    "shortid": "^2.2.14",
    "watchify": "^3.11.1",
    "webpack": "^4.29.6"
  },
  "keywords": [],
  "devDependencies": {
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.0.1",
    "jest": "^24.1.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  }
}
